import java.util.Scanner;

public class Main {

	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		int N,Q;
		while(T>0)
		{
			N = sc.nextInt();
			int[] A = new int[N];
			Q = sc.nextInt();
			int L,R;
			for(int i=0;i<N;i++)
				A[i] = sc.nextInt();
			
			for(int i=0;i<Q;i++)
			{
				L = sc.nextInt();
				R = sc.nextInt();
				int j=0;
				if(A[j])
			}
			T--;
		}
		sc.close();
	}
}
