package March_16_LunchTime;

import java.util.*;

public class ABABAABA {

	public static void main(String[] args){
		
		Scanner sc = new Scanner(System.in);
		int T=Integer.parseInt(sc.nextLine()),index,length;
		String str;
		
		while(T-->0)
		{
			index=-1;
			str=sc.nextLine();
			length=str.length();
		}
		sc.close();
	}
}
