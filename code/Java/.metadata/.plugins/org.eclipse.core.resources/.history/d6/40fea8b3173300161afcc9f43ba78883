package placement_drive;

import java.util.Scanner;

public class T01_Q01 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt(),n,odd,even,ans;
		
		while(t-->0)
		{
			n=sc.nextInt();
			odd=0;
			even=0;
			
			for(int i=1;i<=n;i++)
			{
				if(i%2==0)
					even+=i;
				else
					odd+=i;
			}
			
			
		}
		sc.close();
	}

}
