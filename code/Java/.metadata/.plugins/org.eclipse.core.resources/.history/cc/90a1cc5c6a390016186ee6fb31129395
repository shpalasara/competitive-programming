		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt(),m=sc.nextInt();
		
		
		sc.close();
	}

	public static int find_length_base_seven(int data){
		
		int up = 7,ans=1;
		
		while(data>=up)
		{
			ans++;
			up = up*7;
		}
		
		return ans;
	}
}
